<table class="table table-striped">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |contact|}}
      <tr>
        <td>{{contact.FirstName}}</td>
        <td>{{contact.LastName}}</td>
        <td>
          {{#link-to 'contacts.edit' contact}}Edit{{/link-to}}
        </td>
        <td><a action="delete">Delete</a></td>
      </tr>
      {{else}}
        Sorry, no Contacts here.
    {{/each}}
  </tbody>
</table>

{{outlet}}
